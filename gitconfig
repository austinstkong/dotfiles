[user]
	name = Austin Kong
	email = austinstkong@users.noreply.github.com
[core]
	editor = vim
[color]
	diff = auto
	ui = auto
	status = auto
	branch = auto
	interactive = auto
[push]
	default = simple
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	sb = status -sb
	vimdiff = difftool
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
[diff]
	tool = vimdiff
